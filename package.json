{
  "name": "osintcat",
  "version": "1.0.0",
  "description": "Official JavaScript/TypeScript SDK for OsintCat - the fast, private, and simple OSINT platform for investigations",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest --passWithNoTests",
    "test:coverage": "jest --coverage",
    "clean": "del lib coverage",
    "clean:results": "del results",
    "prepublishOnly": "npm run clean && npm run build && npm test"
  },
  "keywords": [
    "osint",
    "investigation",
    "api-client",
    "osintcat",
    "intelligence",
    "data-collection",
    "breach-search",
    "discord",
    "reddit",
    "github"
  ],
  "author": "OsintCat Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/osintcat/osintcat-js"
  },
  "bugs": {
    "url": "https://github.com/osintcat/osintcat-js/issues"
  },
  "homepage": "https://www.osintcat.net",
  "dependencies": {
    "axios": "^1.6.0",
    "osintcat": "^1.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "files": [
    "lib/**/*",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=16.0.0"
  }
}
